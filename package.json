{
    "name": "nuxt-app",
    "private": true,
    "type": "module",
    "scripts": {
        "format": "prettier . check",
        "format:fix": "prettier . --write",
        "lint": "npx eslint",
        "lint:fix": "npx eslint --fix",
        "prepare": "husky",
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare"
    },
    "lint-staged": {
        "*": "prettier . --write",
        "*{js,vue}": "npx eslint --fix"
    },
    "dependencies": {
        "nuxt": "3.11.2",
        "vue": "3.4.27",
        "vue-router": "4.3.2"
    },
    "devDependencies": {
        "@eslint/js": "9.3.0",
        "eslint": "9.3.0",
        "eslint-plugin-vue": "9.26.0",
        "globals": "15.3.0",
        "husky": "9.0.11",
        "lint-staged": "15.2.5",
        "prettier": "3.2.5"
    }
}
